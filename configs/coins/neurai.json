{
  "coin": {
    "name": "Neurai",
    "shortcut": "XNA",
    "label": "Neurai",
    "alias": "neurai"
  },
  "ports": {
    "backend_rpc": 8069,
    "backend_message_queue": 38369,
    "blockbook_internal": 9069,
    "blockbook_public" : 9169
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-neurai-hw",
    "package_revision": "neurai-dev-hw",
    "system_user": "neurai-hw",
    "version": "1.0.4.0",
    "binary_url": "https://github.com/NeuraiProject/Neurai/releases/download/1.0.4/neurai-1.0.4.0.WITH.NEW.DERIVATION.-x86_64-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "ea2577c3038e740e264b7df1daf43195fee29b2baacb1ec865ee37a925dd60fb",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "neurai-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/neuraid -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "deprecatedrpc": "estimatefee"
    }
  },
  "blockbook": {
    "package_name": "blockbook-neurai-hw",
    "system_user": "blockbook-neurai-hw",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "slip44": 1900,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "Asimov",
    "package_maintainer_email": ""
  }
}
